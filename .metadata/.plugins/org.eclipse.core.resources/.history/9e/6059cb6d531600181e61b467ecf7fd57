package com.arraylist;

interface Parser
{
	String parse(String str);
}

class StringParser
{
	public static String convert(String str)
	{
		if(str.length()<=3)
			str=str.toUpperCase();
		else
			str=str.toLowerCase();
		
		return str;
			
	}
}
